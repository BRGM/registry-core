## $model and $node should be bound to the register
## $members to the list of member entities

      #set($started=false)
      #foreach($itemMember in $lib.reg.asItemList($members))
        #set($item = $itemMember.item)
        #set($member = $itemMember.member)
        #if ($item)
          #set($show = !$item.hasResourceValue("reg:itemClass", "reg:Register") 
               && !$item.hasResourceValue("reg:itemClass", "reg:NamespaceForward")
               && !$item.hasResourceValue("reg:status", "reg:statusReserved"))
        #else
           #set($show=true)
        #end
        #if($show)
          #if(!$started)
            #set($started=true)
            <h3>Members</h3>
          #end

          <div class="result-blob">
            <div class="blob-inner">
              <div class="row">
                <div class="span2">
                  #if($item)
                    <strong>Item:</strong>  <a href="#linkhref($item)" title="$item.uRI">$item.getPropertyValue("reg:notation").lexicalForm</a>
                  #else
                    <strong>Item:</strong>  <a href="#linkhrefInt($member)" title="$member.uRI">$member.name</a>
                  #end
                </div>
                <div class="span2">
                  <strong>Label:</strong> $member.name
                </div>
                <div class="span2">
                  <strong>Type:</strong> #foreach($ty in $member.listPropertyValues("rdf:type"))#linkfor($ty)#if( $foreach.hasNext ), #end#end
                </div>
              #if($item)
                <div class="pull-right">
                  #showstatus($item.getPropertyValue("reg:status"))
                </div>
              #end
            </div>
            <div class="row">
              <div class="span7">
                #description($member, "")
              </div>
            </div>
          </div>
        </div>
        #end
      #end
