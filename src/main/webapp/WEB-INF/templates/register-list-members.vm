## $model and $node should be bound to the register
## $members to the list of member entities

      #set($started=false)
      #foreach($member in $members)
        #set($itemL=$member.connectedNodes("^reg:entity/^reg:definition"))
        #if($itemL.isEmpty())
          #set($show=true)
        #else
          #set($item = $itemL[0])
          #set($show = !$item.hasResourceValue("reg:itemClass", "reg:Register") 
               && !$item.hasResourceValue("reg:itemClass", "reg:NamespaceForward")
               && !$item.hasResourceValue("reg:status", "reg:statusReserved"))
        #end
        #if($show)
          #if(!$started)
            #set($started=true)
            <h3>Members</h3>
          #end

          <div class="result-blob">
            <div class="blob-inner">
              <div class="row">
                <div class="span2">
                  #if($itemL.isEmpty())
                    <strong>Item:</strong> #linkfor($member)
                  #else
                    <strong>Item:</strong>
                      #foreach($item in $itemL)
                        <a href="#linkhref($item)" title="$item.uRI">$item.getPropertyValue("reg:notation").lexicalForm</a>#if( $foreach.hasNext ), #end
                      #end
                  #end
                </div>
                <div class="span2">
                  <strong>Label:</strong> $member.name
                </div>
                <div class="span2">
                  <strong>Type:</strong> #foreach($ty in $member.listPropertyValues("rdf:type"))#linkfor($ty)#if( $foreach.hasNext ), #end#end
                </div>
              #if(!$itemL.isEmpty())
                <div class="pull-right">
                  #showstatus($item.getPropertyValue("reg:status"))
                </div>
              #end
            </div>
            <div class="row">
              <div class="span7">
                #description($member, "")
              </div>
            </div>
          </div>
        </div>
        #end
      #end
