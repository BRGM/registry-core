<_sediment-type-scheme> a reg:RegisterItem;
    reg:definition [
       reg:entity  def-bw:sediment-type ;
    ];
    reg:category taxo:environment;
    .

def-bw:sediment-type
      a       skos:ConceptScheme, void:Dataset ;
      rdfs:comment "A SKOS concept scheme for sediment types found at bathing waters."@en ;
      rdfs:isDefinedBy <http://environment.data.gov.uk/sources/def/bathing-water.ttl> ;
      rdfs:label "sediment-type"@en ;
      skos:hasTopConcept def-bw:sand-sediment , def-bw:rock-sediment , def-bw:other-sediment , def-bw:shingle-sediment , def-bw:mud-sediment , def-bw:marsh-sediment .
